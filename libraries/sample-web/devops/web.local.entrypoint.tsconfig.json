{
  "compilerOptions": {
    "baseUrl": ".",
    "stripInternal": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "moduleResolution": "node",
    "module": "es2015",
    "noEmitOnError": false,
    "outDir": "../__artifacts/local/entrypoint",
    "lib": ["es2015", "dom"],
    "target": "es2015",
    "skipLibCheck": true,
    "strictNullChecks": true,
    "inlineSources": true,
    "inlineSourceMap": true,
    "types": [
      "node",
      "jasmine",
      "systemjs"
    ],
    "paths": {
      "@sharpangles/platform-global": ["../../platform-global/__artifacts/release"]
    }
  },
  "files": [
    "./web.local.entrypoint.ts"
  ]
}
