{
  "version": "1.0.0-alpha.0",
  "name": "@sharpangles/sample-dependency",
  "private": true,
  "description": "Global build bootstraps",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sharpangles/platform.git"
  },
  "keywords": [
    "Angular",
    "Enterprise",
    "Bootstrap"
  ],
  "author": "Eric Nilsen",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sharpangles/platform/issues"
  },
  "homepage": "https://github.com/sharpangles/platform/tree/master/modules/bootstrap#readme",
  "scripts": {
    "build:library": "tsc -p devops/library.dev.tsconfig.json",
    "tsc:app": "tsc -p devops/web.dev.tsconfig.json",
    "rollup:app": "rollup -c devops/web.dev.rollup.config.js",
    "build:app": "npm run tsc:app && npm run rollup:app",
    "copy": "cpy package.json README.md __artifacts/release",
    "build": "npm run build:library && npm run build:app",
    "postbuild": "npm run copy",
    "clean": "rimraf -rf __artifacts"
  },
  "peerDependencies": {
    "@angular/animations": "^4.0.0",
    "@angular/common": "^4.0.0",
    "@angular/compiler": "^4.0.0",
    "@angular/compiler-cli": "^4.0.0",
    "@angular/core": "^4.0.0",
    "@angular/forms": "^4.0.0",
    "@angular/http": "^4.0.0",
    "@angular/platform-browser": "^4.0.0",
    "@angular/platform-browser-dynamic": "^4.0.0",
    "@angular/platform-server": "^4.0.0",
    "@angular/platform-webworker": "^4.0.0",
    "@angular/router": "^4.0.0",
    "@sharpangles/bootstrap": "1.0.0-beta.0",
    "tsconfig-paths": "^2.1.2"
  },
  "devDependencies": {
    "karma": "^1.3.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-jasmine": "^1.0.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "cpy-cli": "^1.0.1",
    "jasmine": "^2.5.3",
    "nyc": "^10.1.2",
    "rimraf": "^2.5.4",
    "rollup": "^0.41.6",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-multi-entry": "^2.0.1",
    "concurrently": "^3.4.0",
    "closure-compiler-cli": "^1.0.2",
    "google-closure-compiler": "^20170409.0.0",
    "google-closure-library": "^20170409.0.0"
  }
}
