{
  "version": "1.0.0-beta.0",
  "name": "@sharpangles/sample-web",
  "private": true,
  "description": "Global build bootstraps",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sharpangles/platform.git"
  },
  "keywords": [
    "Angular",
    "Enterprise",
    "Bootstrap"
  ],
  "author": "Eric Nilsen",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sharpangles/platform/issues"
  },
  "homepage": "https://github.com/sharpangles/platform/tree/master/modules/bootstrap#readme",
  "scripts": {
    "build:entrypoint": "tsc -p tsconfig.entrypoint.json",
    "build:dist": "tsc -p tsconfig.app.json && rollup -c rollup.config.js",
    "build": "npm run build:entrypoint && npm run build:dist && npm run copy",
    "minify:entrypoint": "closure-compiler --compilation_level ADVANCED_OPTIMIZATIONS --create_source_map ./__artifacts/build/entrypoint/customEntryPoint.min.js.map --js node_modules/google-closure-library/closure/goog/base.js --js ./__artifacts/build/entrypoint/customEntryPoint.js --js node_modules/systemjs/dist/system.src.js --language_in ECMASCRIPT6 --language_out ES5_STRICT > ./__artifacts/build/entrypoint/customEntryPoint.min.js",
    "copy": "cpy index.html __artifacts/serve && cpy ../../node_modules/systemjs/dist/system.src.js __artifacts/serve/node_modules/systemjs/dist && cpy ../../node_modules/core-js/es7/reflect.js __artifacts/serve/polyfills && cpy __artifacts/build/entrypoint/* __artifacts/serve/entrypoint"
  },
  "peerDependencies": {
    "@angular/animations": "^4.0.0",
    "@angular/common": "^4.0.0",
    "@angular/compiler": "^4.0.0",
    "@angular/compiler-cli": "^4.0.0",
    "@angular/core": "^4.0.0",
    "@angular/forms": "^4.0.0",
    "@angular/http": "^4.0.0",
    "@angular/platform-browser": "^4.0.0",
    "@angular/platform-browser-dynamic": "^4.0.0",
    "@angular/platform-server": "^4.0.0",
    "@angular/platform-webworker": "^4.0.0",
    "@angular/router": "^4.0.0",
    "@sharpangles/bootstrap": "1.0.0-beta.0",
    "tsconfig-paths": "^2.1.2"
  },
  "devDependencies": {
    "karma": "^1.3.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-jasmine": "^1.0.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "cpy-cli": "^1.0.1",
    "jasmine": "^2.5.3",
    "nyc": "^10.1.2",
    "rimraf": "^2.5.4",
    "rollup": "^0.41.6",
    "concurrently": "^3.4.0",
    "systemjs": "0.19.40",
    "closure-compiler-cli": "1.0.2",
    "google-closure-compiler": "20170409.0.0",
    "google-closure-library": "^20170409.0.0"
  }
}
